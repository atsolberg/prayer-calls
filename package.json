{
  "name": "prayer-calls",
  "version": "1.0.0",
  "description": "App to document prayer calls",
  "main": "src/App.js",
  "scripts": {
    "build": "parcel build",
    "dev": "parcel src/index.html",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "start": "node dist/backend/index.js",
    "startlocal": "npm -s run build && node dist/backend/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && open ./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atsolberg/prayer-calls.git"
  },
  "author": "Aaron Solberg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atsolberg/prayer-calls/issues"
  },
  "homepage": "https://github.com/atsolberg/prayer-calls#readme",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@emotion/babel-plugin": "^11.7.2",
    "@tailwindcss/forms": "^0.5.0",
    "@testing-library/react": "^12.1.4",
    "autoprefixer": "^10.4.2",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "parcel": "^2.3.2",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.0.23"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "js-cookie": "^3.0.1",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "targets": {
    "frontend": {
      "source": [
        "src/index.html"
      ],
      "publicUrl": "/frontend"
    },
    "backend": {
      "source": "server/index.js",
      "optimize": false,
      "context": "node",
      "engines": {
        "node": ">=16"
      }
    }
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./src/setupJest.js"
    ]
  }
}
